{
    "provider": {
        "name": "Alice Service"
    },
    "consumer": {
        "name": "Consumer"
    },
    "interactions": [
        {
            "providerState": "Good Mallory exists",
            "description": "a retrieve Mallory request",
            "request": {
                "method": "GET",
                "path": "/mallory",
                "query": "name=ron&status=good"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "text/html"
                },
                "body": "\"That is some good Mallory.\""
            }
        }
    ],
    "metadata": {
        "pactSpecification": {
            "version": "1.1.0"
        },
        "pact-jvm": {
            "version": "1.0.0"
        }
    }
}
